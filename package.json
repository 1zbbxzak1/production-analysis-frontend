        "@taiga-ui/event-plugins": "^4.7.0",
        "@taiga-ui/icons": "^4.60.0",
        "@taiga-ui/kit": "^4.60.0",
        "@taiga-ui/layout": "^4.60.0",
        "express": "^5.1.0",
        "rxjs": "~7.8.2",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular/build": "^20.3.9",
        "@angular/cli": "^20.3.9",
        "@angular/compiler-cli": "^20.3.10",
        "@types/express": "^5.0.5",
        "@types/jasmine": "~5.1.12",
        "@types/node": "^24.10.0",
        "angular-eslint": "20.5.1",
        "eslint": "^9.39.1",
        "jasmine-core": "~5.12.1",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
