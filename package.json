{
    "name": "production-analysis-frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "serve:ssr": "node dist/production-analysis-frontend/server/server.mjs",
        "lint": "ng lint",
        "docker:build": "docker build -t production-analysis-frontend .",
        "docker:run": "docker run -p 4200:4200 production-analysis-frontend",
        "docker:compose": "docker-compose up -d"
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "overrides": [
            {
                "files": "*.html",
                "options": {
                    "parser": "angular"
                }
            }
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/cdk": "^20.2.12",
        "@angular/common": "^20.3.10",
        "@angular/compiler": "^20.3.10",
        "@angular/core": "^20.3.10",
        "@angular/forms": "^20.3.10",
        "@angular/platform-browser": "^20.3.10",
        "@angular/platform-server": "^20.3.10",
        "@angular/router": "^20.3.10",
        "@angular/service-worker": "^20.3.10",
        "@angular/ssr": "^20.3.9",
        "@ng-web-apis/universal": "^4.12.2",
        "@taiga-ui/addon-charts": "^4.60.0",
        "@taiga-ui/addon-commerce": "^4.60.0",
        "@taiga-ui/addon-doc": "^4.60.0",
        "@taiga-ui/addon-mobile": "^4.60.0",
        "@taiga-ui/addon-table": "^4.60.0",
        "@taiga-ui/cdk": "^4.60.0",
